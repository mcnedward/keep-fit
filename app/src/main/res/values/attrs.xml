<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SettingsView">
        <attr name="setting_title" format="string" />
        <attr name="setting_description" format="string" />
        <attr name="setting_checkable" format="boolean" />
        <attr name="setting_unit" format="enum">
            <enum name="meters" value="1"/>
            <enum name="yards" value="2"/>
            <enum name="miles" value="3"/>
            <enum name="kilometers" value="4"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="StatisticView">
        <attr name="statistics_title" format="string"/>
    </declare-styleable>
    <declare-styleable name="TabSelectionView">
        <attr name="tab_selection_title" format="string"/>
    </declare-styleable>
    <declare-styleable name="StatisticsSelectionView">
        <attr name="statistics_selection_title" format="string"/>
    </declare-styleable>
</resources>